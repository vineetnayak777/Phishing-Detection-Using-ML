{
	"manifest_version": 2,
	
	"name": "Detect Phishing Sites",
	"description": "Detects whether the URL is Phishing or not.",
	"version": "1.0",
	
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "index.html"
	},
	
	"background": {
    "scripts": ["jquery-2.1.3.min.js","main.js"]
	},
	"permissions": [
		"activeTab",
		"webRequest",
		"*://*.google.com/",
		"webRequestBlocking",
		"tabs",
		"<all_urls>"
	]
}